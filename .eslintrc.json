{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
      "no-unused-vars": ["error", { "vars": "none", "args": "after-used", "ignoreRestSiblings": false }]
    }
}
